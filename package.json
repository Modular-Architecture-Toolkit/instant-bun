{
  "name": "@u-tools/core",
  "version": "0.0.63",
  "main": "index.ts",
  "devDependencies": {
    "bun-types": "latest"
  },
  "description": "U Tools is a simple zero-dependency, fast, and lightweight toolkit for Bun",
  "license": "MIT",
  "scripts": {
    "docs-gen": "bun run ./_apps/generator-cli-app/repo-docs-generator.ts",
    "unit-gen": "bun run ./_apps/generator-cli-app/repo-unit-test-gen.ts",
    "docs-server": "bun run ./_apps/docs-server/docs-server.ts",
    "test": "bun test",
    "test:watch": "bun test --watch --coverage",
    "generate-types": "tsc -p tsconfig-types-gen.json",
    "release": "sh ./release.sh",
    "local-release": "LOCAL_RUN=true sh ./release.sh"
  },
  "dependencies": {
    "typescript": "latest"
  }
}
